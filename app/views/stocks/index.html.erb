<div class="table-container px-4 bg-white">
  <h3 class="text-center">Stocks List: <%= @stocks.count %></h3>
  <table class="table table-hover table-responsive-md text">
    <thead>
    <tr>
      <th scope="col">Symbol</th>
      <th scope="col">Name</th>
      <th scope="col">Latest Price</th>
      <th scope="col">Latest Time</th>
    </tr>
    </thead>
    <tbody>
      <% @stocks.each do |stock| %>
        <tr>
          <%# link_to user.email, user %>
          <td class="text"><%= stock.symbol %></td>
          <td class="text"><%= stock.name %></td>
          <td class="text"><%= stock.latest_price %></td>
          <td class="text"><%= stock.latest_time %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br></br>
